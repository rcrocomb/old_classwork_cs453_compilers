void alpha(void)
{
    int a, b;
    if ( a < b);
    if ( a <= b);
    if ( a > b);
    if ( a >= b);
    if ( a == b);
    if ( a != b);
}

void bravo(void)
{
    int a, b;
    while (a < b);
    while (a <= b);
    while (a > b);
    while (a >= b);
    while (a == b);
    while (a != b);
}

void charlie(void)
{
    int a, b;
    for (a = 1; a < b; a = a + 1);
    for (a = 1; a <= b; a = a + 1);
    for (a = 1; a > b; a = a + 1);
    for (a = 1; a >= b; a = a + 1);
    for (a = 1; a == b; a = a + 1);
    for (a = 1; a != b; a = a + 1);
}

void delta(void)
{
    if ( (1 < 2) && (1 < 2));

    if ( (1 <= 2) && (1 <= 2));
    if ( (1 > 2) && (1 > 2));
    if ( (1 >= 2) && (1 >= 2));
    if ( (1 == 2) && (1 == 2));
    if ( (1 != 2) && (1 != 2));

    if ( (1 < 2) && !(1 < 2));
    if ( (1 <= 2) && !(1 <= 2));
    if ( (1 > 2) && !(1 > 2));
    if ( (1 >= 2) && !(1 >= 2));
    if ( (1 == 2) && !(1 == 2));
    if ( (1 != 2) && !(1 != 2));

}

void echo(void)
{
    while ( (1 < 2) && (1 < 2));
    while ( (1 <= 2) && (1 <= 2));
    while ( (1 > 2) && (1 > 2));
    while ( (1 >= 2) && (1 >= 2));
    while ( (1 == 2) && (1 == 2));
    while ( (1 != 2) && (1 != 2));

    while ( !(1 < 2) && (1 < 2));
    while ( !(1 <= 2) && (1 <= 2));
    while ( !(1 > 2) && (1 > 2));
    while ( !(1 >= 2) && (1 >= 2));
    while ( !(1 == 2) && (1 == 2));
    while ( !(1 != 2) && (1 != 2));
}

void foxtrot(void)
{
    int a, b;
    for (a = 1; (1 < 2) && (1 < 2) ;  a = a + 1);
    for (a = 1; (1 <= 2) && (1 <= 2) ;  a = a + 1);
    for (a = 1; (1 > 2) && (1 > 2) ;  a = a + 1);
    for (a = 1; (1 >= 2) && (1 >= 2) ;  a = a + 1);
    for (a = 1; (1 == 2) && (1 == 2) ;  a = a + 1);
    for (a = 1; (1 != 2) && (1 != 2) ;  a = a + 1);

    for (a = 1; !(1 < 2) && (1 < 2) ;  a = a + 1);
    for (a = 1; !(1 <= 2) && (1 <= 2) ;  a = a + 1);
    for (a = 1; !(1 > 2) && (1 > 2) ;  a = a + 1);
    for (a = 1; !(1 >= 2) && (1 >= 2) ;  a = a + 1);
    for (a = 1; !(1 == 2) && (1 == 2) ;  a = a + 1);
    for (a = 1; !(1 != 2) && (1 != 2) ;  a = a + 1);
}

void golf(void)
{
    if ( (1 < 2) || (1 < 2));
    if ( (1 <= 2) || (1 <= 2));
    if ( (1 > 2) || (1 > 2));
    if ( (1 >= 2) || (1 >= 2));
    if ( (1 == 2) || (1 == 2));
    if ( (1 != 2) || (1 != 2));

    if ( (1 < 2) || !(1 < 2));
    if ( (1 <= 2) || !(1 <= 2));
    if ( (1 > 2) || !(1 > 2));
    if ( (1 >= 2) || !(1 >= 2));
    if ( (1 == 2) || !(1 == 2));
    if ( (1 != 2) || !(1 != 2));
}

void hotel(void)
{
    while ( (1 < 2) || (1 < 2));
    while ( (1 <= 2) || (1 <= 2));
    while ( (1 > 2) || (1 > 2));
    while ( (1 >= 2) || (1 >= 2));
    while ( (1 == 2) || (1 == 2));
    while ( (1 != 2) || (1 != 2));

    while ( !(1 < 2) || (1 < 2));
    while ( !(1 <= 2) || (1 <= 2));
    while ( !(1 > 2) || (1 > 2));
    while ( !(1 >= 2) || (1 >= 2));
    while ( !(1 == 2) || (1 == 2));
    while ( !(1 != 2) || (1 != 2));
}

void india(void)
{
    int a, b;
    for (a = 1; (1 < 2) || (1 < 2) ;  a = a + 1);
    for (a = 1; (1 <= 2) || (1 <= 2) ;  a = a + 1);
    for (a = 1; (1 > 2) || (1 > 2) ;  a = a + 1);
    for (a = 1; (1 >= 2) || (1 >= 2) ;  a = a + 1);
    for (a = 1; (1 == 2) || (1 == 2) ;  a = a + 1);
    for (a = 1; (1 != 2) || (1 != 2) ;  a = a + 1);

    for (a = 1; !(1 < 2) || (1 < 2) ;  a = a + 1);
    for (a = 1; !(1 <= 2) || (1 <= 2) ;  a = a + 1);
    for (a = 1; !(1 > 2) || (1 > 2) ;  a = a + 1);
    for (a = 1; !(1 >= 2) || (1 >= 2) ;  a = a + 1);
    for (a = 1; !(1 == 2) || (1 == 2) ;  a = a + 1);
    for (a = 1; !(1 != 2) || (1 != 2) ;  a = a + 1);
}

